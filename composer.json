{
  "name": "dof-dss/maestro",
  "description": "Maestro: NICS Drupal multi-server, multi-site command shell.",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/shiftby/MetaYaml"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "doctrine/cache": "~1.5",
    "guzzlehttp/guzzle": "^5.3",
    "guzzlehttp/ringphp": "^1.1",
    "symfony/console": "^4.2",
    "symfony/finder": "^3.0",
    "symfony/filesystem": "^3.0",
    "symfony/process": "^3.0 >=3.4",
    "stecman/symfony-console-completion": "^0.11",
    "symfony/event-dispatcher": "^4.2",
    "padraic/phar-updater": "^1.0",
    "symfony/dependency-injection": "^4.2",
    "symfony/config": "^4.2",
    "ext-json": "*",
    "composer/ca-bundle": "^1.3",
    "webflo/drupal-finder": "^1.2",
    "paragonie/random_compat": ">=2",
    "league/flysystem": "^2.0",
    "romaricdrigon/metayaml": "dev-master",
    "symfony/yaml": "^4.2"
  },
  "suggest": {
    "drush/drush": "For Drupal projects"
  },
  "autoload": {
    "files": [
      "constants.php"
    ],
    "psr-4": {
      "Maestro\\": "src"
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.2"
  },
  "bin": [
    "bin/maestro"
  ],
  "config": {
    "platform": {
      "php": "7.2"
    },
    "allow-plugins": {
      "bamarni/composer-bin-plugin": true
    }
  }
}
