{
  "name": "dof-dss/maestro-shell",
  "description": "Shell for working with Maestro projects.",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/shiftby/MetaYaml"
    }
  ],
  "require": {
    "symfony/console": "^4.2",
    "symfony/finder": "^4.2",
    "symfony/filesystem": "^4.2",
    "symfony/process": "^4.2",
    "stecman/symfony-console-completion": "^0.11",
    "symfony/event-dispatcher": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/config": "^4.2",
    "ext-json": "*",
    "composer/ca-bundle": "^1.3",
    "romaricdrigon/metayaml": "dev-master",
    "symfony/yaml": "^4.2",
    "dof-dss/maestro-core": "^0.0.8",
    "webflo/drupal-finder": "^1.2"
  },
  "suggest": {
    "drush/drush": "For Drupal projects"
  },
  "autoload": {
    "files": [
      "constants.php"
    ],
    "psr-4": {
      "Maestro\\Shell\\": "src"
    }
  },
  "bin": [
    "bin/maestro"
  ],
  "config": {
    "platform": {
      "php": "8.1"
    }
  }
}
