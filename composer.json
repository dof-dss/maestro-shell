{
  "name": "dof-dss/maestro-shell",
  "description": "Shell for working with Maestro projects.",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/shiftby/MetaYaml"
    }
  ],
  "require": {
    "symfony/console": "^6.0",
    "symfony/finder": "^6.0",
    "symfony/filesystem": "^6.0",
    "symfony/process": "^6.0",
    "stecman/symfony-console-completion": "^0.12",
    "symfony/event-dispatcher": "^6.0",
    "symfony/dependency-injection": "^6.0",
    "symfony/config": "^6.0",
    "ext-json": "*",
    "composer/ca-bundle": "^1.3",
    "romaricdrigon/metayaml": "dev-master",
    "symfony/yaml": "^6.0",
    "dof-dss/maestro-core": "^1.0",
    "webflo/drupal-finder": "^1.2",
    "symfony/http-client": "^5.4",
    "symfony/cache": "^6.3"
  },
  "suggest": {
    "drush/drush": "For Drupal projects"
  },
  "autoload": {
    "files": [
      "constants.php"
    ],
    "psr-4": {
      "Maestro\\Shell\\": "src"
    }
  },
  "bin": [
    "bin/maestro"
  ],
  "config": {
    "platform": {
      "php": "8.1"
    }
  }
}
