{
  "directories": [
    "bin",
    "src"
  ],
  "files": [
    "vendor/autoload.php"
  ],
  "finder": [
    {
      "in": "vendor",
      "name": "*.php*",
      "exclude": [
        "README",
        "CHANGELOG",
        "LICENSE",
        ".editorconfig",
        "bin",
        "doc",
        "build",
        "phpunit",
        "test",
        "tests",
        "Tests"
      ]
    }
  ],
  "output": "build/maestro.phar",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Json",
    "KevinGH\\Box\\Compactor\\Php"
  ]
}